{
  "name": "@react-mool/core",
  "version": "0.1.0",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "homepage": "https://github.com/mozartspa/react-mool#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mozartspa/react-mool.git"
  },
  "bugs": {
    "url": "https://github.com/mozartspa/react-mool/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "yarn build-esm --watch",
    "build": "rimraf ./dist && yarn build-cjs && yarn build-esm",
    "build-cjs": "rimraf ./dist/lib && tsc",
    "build-esm": "rimraf ./dist/esm && tsc --outDir dist/esm --module es2015",
    "lint": "eslint .",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@types/react": "^17.0.13",
    "@types/react-router-dom": "^5.1.7",
    "react": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^16.9.0 || ^17.0.0"
  },
  "dependencies": {
    "@mozartspa/mobx-form": "^0.5.0",
    "@types/dlv": "^1.1.2",
    "@types/node-polyglot": "^2.4.1",
    "dlv": "^1.1.3",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "node-polyglot": "^2.4.0",
    "query-string": "^7.0.1",
    "react-query": "^3.18.1",
    "react-router-dom": "^5.2.0",
    "rooks": "^5.0.2"
  }
}
